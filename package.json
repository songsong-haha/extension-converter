{
  "name": "extension-converter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "codex:precompact": "node scripts/codex/pre-compact.mjs",
    "test:e2e": "playwright test",
    "qa:ai": "node scripts/qa/ai-qa.mjs",
    "qa:gate": "node scripts/qa/qa-gate.mjs",
    "growth:track": "node scripts/qa/track-growth-loop.mjs",
    "agent:create": "node scripts/worktree/create-agent.mjs",
    "agent:create:ga": "node scripts/worktree/create-agent.mjs $npm_config_agent $npm_config_task ${npm_config_base:-main} ga",
    "agent:task:start": "node scripts/worktree/start-growth-task.mjs",
    "agent:teams:start": "node scripts/worktree/start-growth-teams.mjs",
    "agent:merge": "node scripts/worktree/merge-agent.mjs",
    "agent:auto-promote": "node scripts/worktree/auto-promote.mjs",
    "agent:list": "node scripts/worktree/list-agents.mjs",
    "agent:remove": "node scripts/worktree/remove-agent.mjs",
    "agent:remove:branch": "node scripts/worktree/remove-agent.mjs",
    "loop:run": "node scripts/loop/codex-loop.mjs",
    "loop:run:once": "node scripts/loop/codex-loop.mjs --max-iterations 1",
    "loop:run:auto-promote": "node scripts/loop/codex-loop.mjs --auto-promote",
    "loop:bg:start": "node scripts/loop/start-bg.mjs",
    "loop:bg:start:auto-promote": "node scripts/loop/start-bg.mjs --auto-promote",
    "loop:bg:stop": "node scripts/loop/stop-bg.mjs",
    "loop:bg:status": "node scripts/loop/status-bg.mjs",
    "loop:daemon:install": "node scripts/loop/install-launchd.mjs",
    "loop:daemon:uninstall": "node scripts/loop/uninstall-launchd.mjs",
    "loop:daemon:status": "node scripts/loop/status-launchd.mjs",
    "test:loop": "node --test tests/loop-scripts.test.mjs tests/seed-prd-from-backlog.test.mjs"
  },
  "dependencies": {
    "@sentry/nextjs": "^10.38.0",
    "next": "16.1.1",
    "react": "19.2.3",
    "react-dom": "19.2.3"
  },
  "devDependencies": {
    "@playwright/test": "1.56.0",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
